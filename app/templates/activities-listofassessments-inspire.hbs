<div class="fixed-modal paper-modal paper">
  <div class="close glyphicon glyphicon-remove cursor-pointer" {{action 'closeModal' 'popup-modal'}}></div>
  <div id="carousel-example-generic" class="carousel slide" data-ride="carousel">
    <!-- Indicators -->
    <ol class="carousel-indicators">
      <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
      <li data-target="#carousel-example-generic" data-slide-to="1"></li>
      <li data-target="#carousel-example-generic" data-slide-to="2"></li>
    </ol>

    <!-- Wrapper for slides -->
    <div class="carousel-inner">
      <div class="item active">
        <img src="/img/whohas.png" alt="whohas">
        <div class="carousel-caption">
          <a href="http://www.pinterest.com/pin/444800900668582539/" target="_blank">I have, who has? Fraction matching</a>
        </div>
      </div>
      <div class="item">
        <img src="/img/10moreless.png" alt="10moreless">
        <div class="carousel-caption">
          <a href="http://www.pinterest.com/pin/42432421462734460/" target="_blank">10 and 1 more/less. 120 chart</a>
        </div>
      </div>
      <div class="item">
        <img src="/img/paperprotractor.png" alt="paperprotractor">
        <div class="carousel-caption">
          <a href="https://www.youtube.com/watch?v=zFNBEZ9-8t8" target="_blank">Paper protractor by Toys from Trash</a>
        </div>
      </div>
      <div class="item">
        <img src="/img/transformfunction.png" alt="transfromfunction">
        <div class="carousel-caption">
          <a href="http://mathmistakes.org/complex/rules.html" target="_blank">Transforming Functions Demo</a>
        </div>
      </div>
      <div class="item">
        <img src="/img/3act.png" alt="3act">
        <div class="carousel-caption">
          <a href="http://blog.mrmeyer.com/category/3acts/" target="_blank">Dan Meyer 3 Act Lessons</a>
        </div>
      </div>
    </div>

    <!-- Controls -->
    <a class="left carousel-control" href="#carousel-example-generic" role="button" data-slide="prev">
      <span class="glyphicon glyphicon-chevron-left"></span>
    </a>
    <a class="right carousel-control" href="#carousel-example-generic" role="button" data-slide="next">
      <span class="glyphicon glyphicon-chevron-right"></span>
    </a>
  </div>
</div>